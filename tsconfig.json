{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@": [
        "./src"
      ],
      "@utils/*": [
        "src/services/*"
      ],
      "@components/*": [
        "./src/components/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/views/dashboard/index.js"
, "src/views/dashboard/index.jsx", "src/views/dashboard/index.jsx", "src/services/index.js", "src/main.tsx", "src/App.tsx", "src/views/login/index.jsx", "src/views/login/login-form/login-form.jsx", "src/components/home-header/index.jsx", "src/components/home-footer/index.jsx"  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}